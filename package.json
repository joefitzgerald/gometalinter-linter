{
  "name": "gometalinter-linter",
  "description": "A linter package for the Go language that uses gometalinter to run many linters simultaneously.",
  "keywords": [
    "go",
    "golang",
    "linter",
    "gometalinter",
    "gofmt",
    "goimports",
    "vet",
    "golint",
    "gotype",
    "errcheck",
    "ineffassign",
    "deadcode",
    "gocyclo",
    "test",
    "dupl",
    "structcheck",
    "testify",
    "vetshadow",
    "varcheck",
    "defercheck",
    "aligncheck"
  ],
  "main": "./lib/main",
  "version": "0.1.1",
  "repository": "https://github.com/joefitzgerald/gometalinter-linter",
  "license": "Apache-2.0",
  "engines": {
    "atom": ">=1.1.0 <2.0.0"
  },
  "author": "Joe Fitzgerald <jfitzgerald@pivotal.io> (http://github.com/joefitzgerald)",
  "homepage": "https://atom.io/packages/gometalinter-linter",
  "bugs": {
    "url": "https://github.com/joefitzgerald/gometalinter-linter/issues"
  },
  "dependencies": {
    "atom-package-deps": "^3.0.6"
  },
  "devDependencies": {
    "eslint": "^2.0.0",
    "babel-eslint": "^4.1.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1"
  },
  "package-deps": [
    "go-config",
    "linter"
  ],
  "consumedServices": {
    "go-config": {
      "versions": {
        "0.1.0": "consumeGoconfig"
      }
    }
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provide"
      }
    }
  }
}
